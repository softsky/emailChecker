{
    "rules": {
        "no-console": false,
        "semicolon": [
            false,
            "never"
        ],
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "prettier": true
    },
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts",
            "wallaby.js",
            "webpack.config.js"
        ]
    }
}
